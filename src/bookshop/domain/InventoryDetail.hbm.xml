<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="bookshop.domain.InventoryDetail" table="InventoryDetail">
		<id name="id" type="java.lang.Integer">
			<column name="id" />
			<generator class="identity" />
		</id>
		<many-to-one name="book" class="bookshop.domain.Book" lazy="false">
			<column name="bookId" not-null="true" />
		</many-to-one>
		<property name="count" type="java.lang.Integer">
			<column name="count" length="32" not-null="true" />
		</property>
		<many-to-one name="inventory" class="bookshop.domain.Inventory">
			<column name="inventoryId" not-null="true" />
		</many-to-one>
	</class>
</hibernate-mapping>